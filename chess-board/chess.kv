#:kivy 1.0.9
#:import win kivy.core.window

<ChessGame>:    
    s_width : win.Window.width
    s_height : win.Window.height
    Chessboard:
        id: main_board
        center: self.parent.center

<Chessboard>:
    size : self.parent.s_height, self.parent.s_height
    canvas:
        Rectangle:
            pos: self.pos
            size: self.size
            source: 'board.png'
    
